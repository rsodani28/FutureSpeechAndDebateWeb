# Production environment variables
# These will be used during build time on Vercel

# Skip environment validation during build
SKIP_ENV_VALIDATION=1

# Auth
# Temporary AUTH_SECRET for build process - replace in Vercel environment variables
AUTH_SECRET="temporary_auth_secret_for_build_process_only"

# Database
# SQLite won't work in production - you'd need to use a different database
# Using dummy URL for build process
DATABASE_URL="file:./dummy.db"
